- content_tag_for(:tr, wikipage, :class => cycle(:odd, :even)) do
  %td.title= h link_to wikipage.try(:title), admin_wikipage_path(:title => wikipage.title, :id => wikipage.id)
  %td.actions
    = link_to 'History', admin_wikipage_history_path(:title => wikipage.title)
    |  
    = link_to 'Destroy', [:admin, wikipage], :confirm => 'Are you sure?', :method => :delete